{
  "name": "@atls/cli-ui-parts",
  "version": "0.0.6",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@yarnpkg/libui": "3.0.0",
    "figures": "6.1.0",
    "ink": "3.2.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "18.3.3"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "typings": "dist/index.d.ts"
  }
}
